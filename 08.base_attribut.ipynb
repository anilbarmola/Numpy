    "import numpy as np\n",
    "#*****Base attribute******\n",
    "#base attribute returns None if the array owns the data. otherwise the base attribute refers the original object\n",
    "arr=np.array([1,2,3,4,5,6])\n",
    "x=arr.copy()\n",
    "y=arr.view()\n",
    "print(x.base)\n",
    "print(y.base)"
   ]
